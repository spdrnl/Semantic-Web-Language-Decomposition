    %% ============================================
    subgraph "Layer 3: Class Expressions"
        subgraph "Layer 3b: Class Expressions - Restrictions"
            X[owl:Restriction]
        end
    end

    %% ============================================
    %% LAYER 5: ADVANCED CLASS AXIOMS
    %% ============================================
    subgraph "Layer 5: Advanced Class Axioms"
        AG[owl:disjointWith]
        BV[owl:members]
    end

