    %% LAYER 6: PROPERTY AXIOMS
    %% ============================================
    subgraph "Layer 6: Property Axioms"
        AJ["owl:propertyChainAxiom<br/>(Property Composition)"]
        AK[owl:propertyDisjointWith]
        AL[owl:AllDisjointProperties]
        AM["owl:hasKey<br/>(Key Constraint)"]
    end

